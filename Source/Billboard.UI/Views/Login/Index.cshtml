@model dynamic

@{
    ViewBag.Title = "title";
    Layout = "/Views/Shared/_Layout.cshtml";
}


@using (Html.BeginForm())
{

    <p>
        <label>Username</label>
        <input type="text" name="username" />
    </p>

    <p>
        <label>Password</label>
        <input type="password" name="password" />
    </p>

    <input type="submit" value="Login" />
}

